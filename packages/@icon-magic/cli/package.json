{
  "name": "@icon-magic/cli",
  "version": "2.2.4-beta.0",
  "description": "Icon magic CLI package.",
  "main": "dist/src/index.js",
  "author": "Ramitha Chitloor <rchitloor@linkedin.com>",
  "license": "BSD-2-Clause",
  "private": false,
  "scripts": {
    "start": "yarn run watch",
    "watch": "watch 'yarn run test' './src' './test' --wait=1",
    "pretest": "yarn run build & yarn run lint",
    "prepack": "yarn run build",
    "test": "mocha --opts test/mocha.opts dist/test",
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -t msbuild --project . -c tslint.json --fix"
  },
  "bin": {
    "icon-magic": "dist/src/index.js"
  },
  "dependencies": {
    "@icon-magic/build": "^2.2.3-beta.0",
    "@icon-magic/distribute": "^2.2.4-beta.0",
    "@icon-magic/generate": "^2.2.3-beta.0",
    "@icon-magic/logger": "^2.0.1-beta.0",
    "commander": "^5.1.0",
    "config-reader": "^0.1.1"
  }
}
